  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-zacanger": "1.1.3",
    "husky": "4.2.3",
    "lint-staged": "10.0.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.2",
    "sortpack": "2.1.3"
